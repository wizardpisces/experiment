{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "litepack": "bin/litepack.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "ts-node ./src/index.ts",
    "nodemon": "nodemon",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "litepack",
  "author": "刘泽",
  "module": "dist/litepack.esm.js",
  "size-limit": [
    {
      "path": "dist/litepack.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/litepack.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/etag": "^1.8.0",
    "@types/koa": "^2.13.3",
    "@types/koa-static": "^4.0.1",
    "@vue/compiler-sfc": "^3.1.1",
    "es-module-lexer": "^0.6.0",
    "esbuild": "^0.12.9",
    "etag": "^1.8.1",
    "husky": "^6.0.0",
    "koa": "^2.13.1",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "rollup": "^2.52.1",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
